package supervisedLearning;

import java.util.ArrayList;

import supervisedLearning.data.Age;
import supervisedLearning.data.Class_Label;
import supervisedLearning.data.FavAnimal;
import supervisedLearning.data.FavColor;
import supervisedLearning.data.FavSQLServ;
import supervisedLearning.data.OS;
import supervisedLearning.data.progLangs;
import supervisedLearning.id3.ID3Object;
import supervisedLearning.id3.iClass_Label;

public class Survey2014 implements ID3Object{

	@Override
	public ArrayList<Object> getAttributeList() {
		ArrayList<Object> attributes = new ArrayList<Object>();
		attributes.add(Integer.class);
		attributes.add(Integer.class);
		attributes.add(Integer.class);
		attributes.add(OS.class);
		attributes.add(progLangs.class);
		attributes.add(Integer.class);
		attributes.add(FavAnimal.class);
		attributes.add(Boolean.class);
		attributes.add(Boolean.class);
		attributes.add(FavColor.class);
		attributes.add(Boolean.class);
		attributes.add(Boolean.class);
		attributes.add(Boolean.class);
		attributes.add(FavSQLServ.class);
		attributes.add(Boolean.class);
		return attributes;
	}

	@Override
	public Object getAttributeValue(Object Attribute) {
		if(Attribute.equals(Class_Label.class))
		{
			return this.m_Class;
		}
		
		if(Attribute.equals(Cap_Shape.class))
		{
			return this.m_cap_shape;
		}
		if(Attribute.equals(Cap_Surface.class))
		{
			return this.m_cap_surface;
		}
		if(Attribute.equals(Cap_Color.class))
		{
			return this.m_cap_color;
		}
		if(Attribute.equals(Bruises.class))
		{
			return this.m_bruises;
		}
	}

	@Override
	public iClass_Label getClasslabel() {
		// TODO Auto-generated method stub
		return null;
	}

	@Override
	public void setClasslabel(iClass_Label classLabel) {
		// TODO Auto-generated method stub
		
	}

}
